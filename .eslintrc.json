{
  "env": {
      "es2021": true,
      "node": true
  },
  "extends": [
      "airbnb-base",
      "plugin:prettier/recommended"
  ],
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "import/no-dynamic-require": "off",
    "func-names": "off",
    "no-unused-vars":"off",
    "global-require":"off",
    "prettier/prettier": "error"
  },
  "plugins": [
    "prettier"
  ]
}
